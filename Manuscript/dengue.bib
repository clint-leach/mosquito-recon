keywords = {Absenteeism,Adolescent,Adult,Age Factors,Aged,Aged, 80 and over,Antibodies, Viral,Antibodies, Viral: blood,Child,Child, Preschool,Cohort Studies,Dengue,Dengue Virus,Dengue Virus: immunology,Dengue: epidemiology,Dengue: transmission,Female,Fever of Unknown Origin,Fever of Unknown Origin: epidemiology,Geography,Humans,Incidence,Longitudinal Studies,Male,Middle Aged,Peru,Peru: epidemiology,Seroepidemiologic Studies,Young Adult},
