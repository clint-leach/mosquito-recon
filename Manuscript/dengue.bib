keywords = {ODE inference,dengue fever,deterministic chaos,epidemiology,influenza,likelihood,master equation},
mendeley-tags = {ODE inference},
number = {2},
pages = {156--69},
pmid = {23565331},
title = {{Dynamic noise, chaos and parameter estimation in population biology.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3293205{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {2},
year = {2012}
}
@article{Sattenspiel1995,
abstract = {A model for the spread of infectious diseases among discrete geographic regions is presented that incorporates a mobility process that describes how contact occurs between individuals from different regions. The general formulation of the mobility process is described, and it is shown that the formulation encapsulates a range of mobility behavior from complete isolation of all regions (no mobility) to permanent migration between regions. It is then shown how this mobility process fits into an SIR epidemic model, and two examples are given extending its use. The examples include a model for disease transmission in a population with two distinct mobility patterns operating and a model developed to describe a 1984 measles epidemic on the Caribbean island of Dominica.},
annote = {Develops a general framework for incorporating mobility into epidemic models based on departure rates, destination probabilities, and return rates. They use these rates to explicitly model (deterministic, continuous) movement, along with transmission, with transmission rates summing over all possible neighborhoods, and accounting for effective population size. Accounts for both acquisition of infection in other neighborhoods, and transmission of infection to other neighborhoods (i.e. no infecteds stay home assumption).

Provides a comprehensive, flexible framework that could handle many different assumptions about movement, but needs [n compartments] x [regions]{\^{}}2 equations, so might not scale up very easily.},
author = {Sattenspiel, L and Dietz, K},
