journal = {Environ. Health Perspect.},
keywords = {Bayes Theorem,Dengue,Dengue: epidemiology,Dengue: transmission,Hot Temperature,Humans,Incidence,Poisson Distribution,Queensland,Queensland: epidemiology,Rain,Regression Analysis,Seasons,Socioeconomic Factors,Travel},
month = {feb},
number = {2},
pages = {260--6},
pmid = {22015625},
title = {{Spatial patterns and socioecological drivers of dengue fever transmission in Queensland, Australia.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3279430{\&}tool=pmcentrez{\&}rendertype=abstract},
