keywords = {Aedes,Animals,Brazil,Brazil: epidemiology,Demography,Dengue,Dengue: economics,Dengue: epidemiology,Disease Outbreaks,Disease Outbreaks: economics,Disease Outbreaks: statistics {\&} numerical data,Humans,Incidence,Insect Vectors,Population Surveillance,Risk Assessment,Risk Factors,Sanitation,Socioeconomic Factors,Urban Health,Urban Health: statistics {\&} numerical data,Vulnerable Populations,Vulnerable Populations: statistics {\&} numerical dat},
month = {may},
number = {3},
