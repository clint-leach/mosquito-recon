keywords = {Communicable Diseases,Communicable Diseases: epidemiology,Communicable Diseases: transmission,Computer Simulation,Disease Outbreaks,Group Structure,Humans,Models, Theoretical,Population Density,Population Dynamics},
month = {aug},
number = {32},
pages = {11157--62},
pmid = {16055564},
title = {{Multiscale, resurgent epidemics in a hierarchical metapopulation model.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1183543{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {102},
year = {2005}
}
@article{Stollenwerk2012,
abstract = {We revisit the parameter estimation framework for population biological dynamical systems, and apply it to calibrate various models in epidemiology with empirical time series, namely influenza and dengue fever. When it comes to more complex models such as multi-strain dynamics to describe the virus-host interaction in dengue fever, even the most recently developed parameter estimation techniques, such as maximum likelihood iterated filtering, reach their computational limits. However, the first results of parameter estimation with data on dengue fever from Thailand indicate a subtle interplay between stochasticity and the deterministic skeleton. The deterministic system on its own already displays complex dynamics up to deterministic chaos and coexistence of multiple attractors.},
annote = {Looks at filtering approaches to estimating parameters in models with population noise. They start with a simple linear model that can be solved analytically and then move on to more complex models that require simulation to approximate the likelihood. They provide a short, and slightly confusing overview of particle filtering, and apply it to a seasonally forced SIRS model for influenza.  It seems to work well when fitting to (simulated?) data, and is able to provide good parameter estimates.  They propose applying these methods to a two-serotype dengue model (but it seems that paper never came out).},
author = {Stollenwerk, N and Aguiar, M and Ballesteros, S and Boto, J and Kooi, B and Mateus, L},
doi = {10.1098/rsfs.2011.0103},
file = {:home/clint/Documents/Literature/Stollenwerk et al. - 2012.pdf:pdf},
