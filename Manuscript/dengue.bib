issn = {0007-4853},
journal = {Bull. Entomol. Res.},
keywords = {dengue parameters},
mendeley-tags = {dengue parameters},
number = {01},
pages = {35--39},
title = {{The frequency of host biting and its effect on oviposition and survival in Aedes aegypti (Diptera: Culicidae)}},
volume = {89},
year = {1999}
